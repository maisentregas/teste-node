  "bugs": {
    "url": "https://github.com/MarcosMarchado/teste-node/issues"
  },
  "homepage": "https://github.com/MarcosMarchado/teste-node#readme",
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^0.21.12",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.2"
  }
